{
  "name": "zk-citizen",
  "version": "1.0.0",
  "description": "Privacy-preserving identity (zk-Passport) and census (zk-Census) platform for Network States",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc && npm run build:contracts",
    "build:contracts": "cd contracts && npm run build",
    "test": "jest",
    "test:contracts": "cd contracts && npm test",
    "dev": "vite",
    "dev:server": "ts-node src/server/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "keywords": [
    "zero-knowledge",
    "zk-proofs",
    "privacy",
    "identity",
    "census",
    "mina",
    "zcash",
    "network-state"
  ],
  "author": "ZK-Citizen Team",
  "license": "MIT",
  "dependencies": {
    "o1js": "^1.0.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.0",
    "ethers": "^6.9.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.3.2",
    "ts-node": "^10.9.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "vite": "^5.0.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0"
  }
}
